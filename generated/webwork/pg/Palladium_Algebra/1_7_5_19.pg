Context('Numeric');
$m = random(1, 9);
$x[0] = random(1, 9);
$x[1] = $x[0] + 1;
$y[0] = random(1, 9);
$y[1] = $y[0] + $m;
$f = Formula("$m (x - $x[0]) + $y[0]");
$ymax = max(10, max(@y) + 1);

$image_1 = createLaTeXImage();
$image_1->addToPreamble(latexImagePreamble());
$image_1->BEGIN_LATEX_IMAGE
\begin{tikzpicture}
  \begin{axis}[
      xmin=-1,
      xmax=10,
      ymin=-1,
      ymax=$ymax,
      xtick={2,4,...,8},
      minor xtick={1,...,9},
      ytick={5,10,...,$ymax},
      minor ytick={1,...,$ymax},
      width=0.33\orccaprintwidth
    ]
    \addplot+[-,domain=-1:10,samples=2] {$f};
  \end{axis}
\end{tikzpicture}

END_LATEX_IMAGE

