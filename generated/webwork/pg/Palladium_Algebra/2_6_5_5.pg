Context()->variables->are(a=>'Real',b=>'Real',c=>'Real');
($a,$c) = (1..5)[NchooseK(5,2)];
$b = list_random(4..9);
$d = Formula("b^2-4ac");
$answer = $d->eval(a=>$a,b=>$b,c=>$c);
Context()->flags->set(reduceConstants=>0);
$substituted = $d->substitute(a=>$a,b=>$b,c=>$c);
$b2 = $b**2;
$ac4 = 4*$a*$c;
$solutionstep = Formula("$b2-$ac4");

############################################################
