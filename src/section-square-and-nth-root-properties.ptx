    <exercisegroup cols="5">
      <title>Higher Index Roots</title>
      <introduction>
        <p>
          Simplify the higher index radical.
        </p>
      </introduction>
      <!-- root(n,x) with positive x, integer answers -->
      <!-- seed set to guarantee distinct versions -->
      <exercise label="simplify-radical-positive-integer">
        <webwork seed="1" xml:id="webwork-simplify-radical-positive-integer">
            <pg-code>
              Context("Form");
              do {
                $n = random(3,6,1);
                $r = random(2,5,1);
                $x = $r**($n);
              } until ($x &lt;= 125);
              $expression = Formula("root($n,$x)");
              $answer = Formula("$r");
              $explanation = Formula("$r" . ('*' . "$r") x ($n-1));
              $explanation2 = Formula("$r ^ $n");
            </pg-code>
          <statement>
            <p>
              <m><var name="$expression"/></m>.
            </p>
            <instruction>To enter <m>\sqrt[n]{x}</m>, type <c>root(n, x)</c>. If the value is not a real number, enter <c>not a real number</c>.</instruction>
            <p>
              <var name="$answer" width="30"/>
            </p>
          </statement>
          <solution>
            <p>
              Since <m><var name="$explanation"/> = <var name="$x"/></m>, then <m><var name="$expression"/>=<var name="$answer"/></m>. Or if you prefer, since <m><var name="$explanation2"/> = <var name="$x"/></m>, then <m><var name="$expression"/>=<var name="$answer"/></m>.
            </p>
          </solution>
        </webwork>
      </exercise>
      <exercise label="simplify-radical-positive-integer-copy">
        <webwork seed="2" copy="webwork-simplify-radical-positive-integer"/>
      </exercise>
      <exercise label="simplify-radical-positive-integer-copy2">
        <webwork seed="3" copy="webwork-simplify-radical-positive-integer"/>
      </exercise>
      <exercise label="simplify-radical-positive-integer-copy3">
        <webwork seed="5" copy="webwork-simplify-radical-positive-integer"/>
      </exercise>
      <exercise label="simplify-radical-positive-integer-copy4">
        <webwork seed="6" copy="webwork-simplify-radical-positive-integer"/>
      </exercise>
      <exercise label="simplify-radical-positive-integer-copy5">
        <webwork seed="9" copy="webwork-simplify-radical-positive-integer"/>
      </exercise>
      <!-- root(n,x) with negative x, integer or nonreal answers -->
      <exercise label="simplify-radical-negative-integer">
        <webwork xml:id="webwork-simplify-radical-negative-integer">
            <pg-code>
              Context("Form");
              do {
                $n = random(3,6,1);
                $r = random(2,5,1);
                $x = -$r**($n);
              } until (abs($x) &lt;= 125);
              $expression = Formula("root($n,$x)");
              $answer = ($n % 2 == 1) ? Formula("-$r") : String('not a real number');
              $explanation = Formula("$r" . ('*' . "$r") x ($n-1));
              $explanation2 = Formula("$r ^ $n");
              if ($n % 2 == 1) {
                $th = ($n == 3) ? 'rd' : 'th';
                $explanationText =
                  "Since $x is negative and $n is odd, a ${n}${th} root of $x exists and is equal to $r.";
              }
              else {
                $explanationText =
                  "Since $x is negative and $n is even, this is not a real number.";
              }
            </pg-code>
          <statement>
            <p>
              <m><var name="$expression"/></m>.
            </p>
            <instruction>To enter <m>\sqrt[n]{x}</m>, type <c>root(n, x)</c>. If the value is not a real number, enter <c>not a real number</c>.</instruction>
            <p>
              <var name="$answer" width="30"/>
            </p>
          </statement>
          <solution>
            <p>
              <var name="$explanationText"/>
            </p>
          </solution>
        </webwork>
      </exercise>
      <exercise label="simplify-radical-negative-integer-copy">
        <webwork copy="webwork-simplify-radical-negative-integer"/>
      </exercise>
      <!-- seed set to guarantee an even index -->
      <exercise label="simplify-radical-negative-integer-copy2">
        <webwork seed="4" copy="webwork-simplify-radical-negative-integer"/>
      </exercise>
      <!-- seed set to guarantee an even index -->
      <exercise label="simplify-radical-negative-integer-copy3">
        <webwork seed="5" copy="webwork-simplify-radical-negative-integer"/>
      </exercise>
      <exercise label="simplify-radical-negative-integer-copy4">
        <webwork copy="webwork-simplify-radical-negative-integer"/>
      </exercise>
      <exercise label="simplify-radical-negative-integer-copy5">
        <webwork copy="webwork-simplify-radical-negative-integer"/>
      </exercise>
      <!-- root(n,x) with positive integer x, non-integer answers -->
      <exercise label="simplify-radical-non-integer">
        <webwork xml:id="webwork-simplify-radical-non-integer">
            <pg-code>
              Context("Form");
              do {
                $n = random(3,6,1);
                $r = random(2,5,1);
                $x = $r**($n);
                $y = ($n % 2 == 0) ? list_random(2,3,5..7,10,11) : list_random(2..7,9..12);
                $X = $x*$y;
              } until ($x &lt;= 125 and $X &lt;= 200);
              $expression = Formula("root($n,$X)");
              $answer = Formula("$r root($n,$y)");
            </pg-code>
          <statement>
            <p>
              <m><var name="$expression"/></m>.
            </p>
            <instruction>To enter <m>\sqrt[n]{x}</m>, type <c>root(n, x)</c>. If the value is not a real number, enter <c>not a real number</c>.</instruction>
            <p>
              <var name="$answer" width="30"/>
            </p>
          </statement>
          <solution>
            <p>
              <md>
                <mrow><var name="$expression"/>\amp = \sqrt[<var name="$n"/>]{<var name="$x"/>\cdot <var name="$y"/>}</mrow>
                <mrow>\amp = \sqrt[<var name="$n"/>]{<var name="$x"/>}\cdot \sqrt[<var name="$n"/>]{<var name="$y"/>}</mrow>
                <mrow>\amp = <var name="$answer"/></mrow>
              </md>
            </p>
          </solution>
        </webwork>
      </exercise>
      <exercise label="simplify-radical-non-integer-copy">
        <webwork copy="webwork-simplify-radical-non-integer"/>
      </exercise>
      <exercise label="simplify-radical-non-integer-copy2">
        <webwork copy="webwork-simplify-radical-non-integer"/>
      </exercise>
      <exercise label="simplify-radical-non-integer-copy3">
        <webwork copy="webwork-simplify-radical-non-integer"/>
      </exercise>
      <exercise label="simplify-radical-non-integer-copy4">
        <webwork copy="webwork-simplify-radical-non-integer"/>
      </exercise>
      <exercise label="simplify-radical-non-integer-copy5">
        <webwork copy="webwork-simplify-radical-non-integer"/>
      </exercise>
      <!-- root(n,x) with positive fraction x, denominator is nth power -->
      <exercise label="simplify-radical-denominator-integer">
        <webwork xml:id="webwork-simplify-radical-denominator-integer">
            <pg-code>
              Context("Form");
              do {
                $n = random(3,6,1);
                $r = random(2,5,1);
                $x = $r**($n);
                do {$y = ($n % 2 == 0) ? list_random(2,3,5..7,10,11) : list_random(2..7,9..12);} until (gcd($x,$y) == 1);
              } until ($x &lt;= 125);
              $expression = Formula("root($n,$y/$x)");
              $answer = Formula("1/$r root($n,$y)");
            </pg-code>
          <statement>
            <p>
              <m><var name="$expression"/></m>.
            </p>
            <instruction>To enter <m>\sqrt[n]{x}</m>, type <c>root(n, x)</c>. If the value is not a real number, enter <c>not a real number</c>.</instruction>
            <p>
              <var name="$answer" width="30"/>
            </p>
          </statement>
          <solution>
            <p>
              <md>
                <mrow><var name="$expression"/>\amp = \frac{\sqrt[<var name="$n"/>]{<var name="$y"/>}}{\sqrt[<var name="$n"/>]{<var name="$x"/>}}</mrow>
                <mrow>\amp = \frac{\sqrt[<var name="$n"/>]{<var name="$y"/>}}{<var name="$r"/>}</mrow>
                <mrow>\amp = \frac{1}{<var name="$r"/>}\sqrt[<var name="$n"/>]{<var name="$y"/>}</mrow>
              </md>
            </p>
          </solution>
        </webwork>
      </exercise>
      <exercise label="simplify-radical-denominator-integer-copy">
        <webwork copy="webwork-simplify-radical-denominator-integer"/>
      </exercise>
      <!-- root(n,x) with positive fraction x, denominator is nth power, numerator has nth power factor -->
      <exercise label="simplify-radical-denominator-integer-numerator-splits">
        <webwork xml:id="webwork-simplify-radical-denominator-integer-numerator-splits">
            <pg-code>
              Context("Form");
              do {
                $n = random(3,6,1);
                do {
                  $r = random(2,5,1);
                  $R = random(2,5,1);
                } until (gcd($r,$R) == 1);
                $x = $r**($n);
                $z = $R**($n);
                do {$y = ($n % 2 == 0) ? list_random(2,3,5..7,10,11) : list_random(2..7,9..12);} until (gcd($x,$y) == 1 and gcd($z,$y) == 1);
                $Y = $z*$y;
              } until ($x &lt;= 125 and $z &lt;= 125 and $Y &lt;= 200);
              $expression = Formula("root($n,$Y/$x)");
              $answer = Formula("$R/$r root($n,$y)");
            </pg-code>
          <statement>
            <p>
              <m><var name="$expression"/></m>.
            </p>
            <instruction>To enter <m>\sqrt[n]{x}</m>, type <c>root(n, x)</c>. If the value is not a real number, enter <c>not a real number</c>.</instruction>
            <p>
              <var name="$answer" width="30"/>
            </p>
          </statement>
          <solution>
            <p>
              <md>
                <mrow><var name="$expression"/>\amp = \frac{\sqrt[<var name="$n"/>]{<var name="$Y"/>}}{\sqrt[<var name="$n"/>]{<var name="$x"/>}}</mrow>
                <mrow>\amp = \frac{\sqrt[<var name="$n"/>]{<var name="$z"/>\cdot <var name="$y"/>}}{<var name="$r"/>}</mrow>
                <mrow>\amp = \frac{\sqrt[<var name="$n"/>]{<var name="$z"/>}\sqrt[<var name="$n"/>]{<var name="$y"/>}}{<var name="$r"/>}</mrow>
                <mrow>\amp = \frac{<var name="$R"/>\sqrt[<var name="$n"/>]{<var name="$y"/>}}{<var name="$r"/>}</mrow>
                <mrow>\amp = \frac{<var name="$R"/>}{<var name="$r"/>}\sqrt[<var name="$n"/>]{<var name="$y"/>}</mrow>
              </md>
            </p>
          </solution>
        </webwork>
      </exercise>
      <exercise label="simplify-radical-denominator-integer-numerator-splits-copy">
        <webwork copy="webwork-simplify-radical-denominator-integer-numerator-splits"/>
      </exercise>
      <exercise label="simplify-radical-denominator-integer-numerator-splits-copy2">
        <webwork copy="webwork-simplify-radical-denominator-integer-numerator-splits"/>
      </exercise>
      <exercise label="simplify-radical-denominator-integer-numerator-splits-copy3">
        <webwork copy="webwork-simplify-radical-denominator-integer-numerator-splits"/>
      </exercise>
    </exercisegroup>

