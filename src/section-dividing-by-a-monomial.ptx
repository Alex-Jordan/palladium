              @m = random_subset(4, 2 .. 20);
              push @m, $m[0] + $m[1], $m[2] + $m[3];
              $given = Formula("($b[0] $x^$m[4] $y^$m[5])/($b[1] $x^$m[1] $y^$m[3])");
              $answer = Formula("$b $x^$m[0] $y^$m[2]");
            </pg-code>
            <statement>
