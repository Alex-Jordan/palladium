  <exercise label="rate-of-change-graph-neg-copy2">
    <introduction>
      <p>
        Find the rate of change between the two given points.
      </p>
    </introduction>
    <!-- not an actual copy so that the graph is sized better -->
    <webwork xml:id="webwork-rate-of-change-graph-neg-copy2">
      <pg-code>
        Context("Point");
        do {
          @a = random_subset(4, -5 .. 5)
        } until (min(@a) &lt; 0 &amp;&amp; max(@a) > 0);
        $A = Point((@a)[0,1]);
        $B = Point((@a)[2,3]);
        Context("Fraction");
        $m = Fraction($a[3] - $a[1], $a[2] - $a[0]);
        Context()->flags->set(reduceConstants => 0);
        $top = Formula("x - $a[1]");
        $top = $top->substitute(x => $a[3]);
        $bottom = Formula("x - $a[0]");
        $bottom = $bottom->substitute(x => $a[2]);
      </pg-code>
      <statement>
        <image width="47%">
          <latex-image>
            \begin{tikzpicture}
              \begin{axis}[
                  xmin=-6,
                  ymin=-6,
                  xmax=6,
                  ymax=6,
                  xtick=,
                  ytick=,
                  minor xtick=,
                  minor ytick=,
                  width=0.47\orccaprintwidth,
                ]
                \addplot[soliddot] coordinates {$A $B};
              \end{axis}
            \end{tikzpicture}
          </latex-image>
        </image>
        <p>
          <var name="$m" width="5"/>
        </p>
      </statement>
      <solution>
        <p>
          The coordinates of the points are <m><var name="$A"/></m> and <m><var name="$B"/></m>.
          We apply the rate of change formula:
          <md>
            <mrow>\amp\frac{y_2-y_1}{x_2-x_1}</mrow>
            <mrow>\amp= \frac{<var name="$top"/>}{<var name="$bottom"/>}</mrow>
            <mrow>\amp= \frac{<var name="$top->reduce"/>}{<var name="$bottom->reduce"/>}</mrow>
          </md>
          So the rate of change is <m><var name="$m"/></m>.
        </p>
      </solution>
    </webwork>
  </exercise>
